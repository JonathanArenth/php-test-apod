{% extends "base.html.twig" %}
{% block body %}
<div class="user">
    <img src="{{avatar}}" alt="avatar" class="avatar">
    <div>{{user}}</div>
</div>
<div class="pageContainer">
    <div class="card">
        <div class="title">
            <h2>{{title}}</h2>
            <div>{{date}}</div>
        </div>
        <div class="blockImage">
            <img src="{{image}}" alt="image" class="image">
        </div>
        <div>{{explanation}}</div>
    </div>
</div>
{% endblock %}
